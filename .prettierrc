{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 120,
  "bracketSpacing": true,
  "singleQuote": false,
  "trailingComma": "none",
  "semi": true,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "bracketSameLine": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "proseWrap": "always",
  "quoteProps": "as-needed"
}
